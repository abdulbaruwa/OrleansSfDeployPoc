      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Orleans.Core" Version="2.3.0" />
    <PackageReference Include="Microsoft.Orleans.Core.Abstractions" Version="2.3.0" />
    <PackageReference Include="Microsoft.Orleans.Core.Legacy" Version="2.3.0" />
    <PackageReference Include="Microsoft.Orleans.Hosting.ServiceFabric" Version="2.3.0" />
    <PackageReference Include="Microsoft.Orleans.OrleansAzureUtils" Version="2.3.0" />
    <PackageReference Include="Microsoft.Orleans.Persistence.AzureStorage" Version="2.3.0" />
    <PackageReference Include="Microsoft.Orleans.Runtime.Legacy" Version="2.3.0" />
    <PackageReference Include="Microsoft.ServiceFabric.Services" Version="3.2.187" />
    <PackageReference Include="OrleansDashboard" Version="2.1.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Crm.V2.Implementations\Crm.V2.Implementations.csproj" />
  </ItemGroup>

</Project>
